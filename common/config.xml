<?xml version="1.0" encoding="UTF-8"?>
<axion>
	<debug>
		<level>1</level>
		<usefirephp>0</usefirephp>
	</debug>
	<connections>
		<databases>
			<development>
				<driver>mysql</driver>
				<host>localhost</host>
				<port>3306</port>
				<username></username>
				<password></password>
				<database></database>
				<autocommit>false</autocommit>
			</development>
			<deployment>
				<driver>mysql</driver>
				<host>localhost</host>
				<port>3306</port>
				<username></username>
				<password></password>
				<database></database>
				<autocommit>false</autocommit>
			</deployment>
		</databases>
		<memcached>
			<servers>
				<host>192.168.0.200</host>
				<port>11211</port>
				<weight>1</weight>
			</servers>
			<servers>
				<host>192.168.0.3</host>
				<port>11211</port>
				<weight>2</weight>
			</servers>
		</memcached>
	</connections>
	<dispatcher>
		<class>AXION_DISPATCHER_DEFAULT</class>
	</dispatcher>
	<method>
		<default>html</default>
	</method>
	<module>
		<default>index</default>
	</module>
	<controller>
		<default>index</default>
	</controller>
	<model>
	</model>
	<view>
		<compress>gzip</compress>
		<striphtml>false</striphtml>
		<templatesuffix>tpl</templatesuffix>
	</view>
	<session>
		<handler>memcached</handler>
		<timeout>10800</timeout>
		<savepath></savepath>
	</session>
	<cache>
		<path></path>
		<handler>file</handler>
		<expire>60</expire>
		<prefix>axion</prefix>
		<section>application</section>
	</cache>
	<misc>
		<timezone>Asia/Shanghai</timezone>
	</misc>
</axion>